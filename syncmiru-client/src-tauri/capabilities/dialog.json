{
  "$schema": "./schemas/desktop-schema.json",
  "identifier": "dialog-capability",
  "description": "tauri-plugin-dialog",
  "windows": ["main"],
  "permissions": [
    "dialog:default",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "dialog:allow-message",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:deny-ask",
    "dialog:deny-confirm",
    "dialog:deny-message",
    "dialog:deny-open",
    "dialog:deny-save"
  ]
}
